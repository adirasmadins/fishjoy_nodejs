<!DOCTYPE html>
<html lang="en">
<head>
	    <meta charset="UTF-8">
	    <meta http-equiv="X-UA-Compatible" content="IE=edge">
	    <meta name="viewport" content="width=device-width, initial-scale=1">
	    <title><%=t1016;%></title>
	    <link rel="stylesheet" href="./../../css/bootstrap.css">
	    <link rel="stylesheet" type="text/css" href="./../../css/bootstrap-datetimepicker.min.css">
	    <link rel="stylesheet" href="./../../css/commonPC.css">
	    <link rel="stylesheet" href="./../../css/definePC.css">
		 <!--[if lt IE 9]>
		   <script src="./../../js/html5shiv.min.js"></script>
		   <script src="./../../js/respond.min.js"></script>
		 <![endif]-->
		 <script src="./../../js/jquery-1.11.3.js"></script>
		 <script src="./../../js/bootstrap.js"></script>
		 <script src="./../../js/bootstrap-datetimepicker.js"></script>
		 <script src="./../../js/arttemplate.js"></script>
		 <script src="./../../js/echarts.common.min.js"></script>
		 <script src="./../../js/child_define.js"></script>
</head>
<body>

	<div class="content marginT30">
		<div class="mainbox homebox">
			<div class="panel panel-default">
			  <div class="panel-heading"><%=t1096;%></div>
			  <div class="panel-body tab-content">
	  	       	<div class="tablebox">
	  	   			<table class="table table-bordered table-striped" style="width: 800px">
<!-- 	  	   				<thead class="bgf5">
	  	   					<tr>
	  	   						<th>提现总开关</th>
	  	   						<th width="300">开关</th>
	  	   					</tr>
	  	   				</thead> -->
	  	   			  <tbody id="condition_body">
	  	   			  </tbody>
	  	   			</table>
              <script type="text/html" id="condition_box">
                  {{each data as val}}

                    <tr>
                      {{if val.type == 1}}
                      <td>{{val.desc}}</td>
                      <td>
                        <table class="edit-mainbox">
                          <tbody>
                            <tr>
                              <td>{{val.value}}</td>
                              <td><input type="text" name="" class="form-control eidt-input hide" value=""></td>
                              <td class="textright"><a href="javascript:;" class="btn btn-sm btn-warning eidts-btn" data-key="{{val.key}}"><%=t1631;%></a></td>
                            </tr>
                          </tbody>
                        </table>
                      </td>
                      {{else}}
                      <td>{{val.desc}}</td>
                      <td>
                        <label class="radio-inline">
                          <input type="radio" name="{{val.key}}" value="1" {{val.value == 1 ? 'checked' : ''}}> {{if val.key == 100}}<%=t1118;%> {{else}}<%=t1323;%>{{/if}}
                        </label>
                        <label class="radio-inline">
                          <input type="radio" name="{{val.key}}" value="0" {{val.value == 0 ? 'checked' : ''}}> {{if val.key == 100}}<%=t1119;%> {{else}}<%=t1324;%>{{/if}}
                        </label>
                      </td>
                      {{/if}}
                    </tr>
                  {{/each}}
               </script>
	  	   		</div>
			  </div>
			</div>
		</div>
	</div>
	<script type="text/javascript">
		var namebox = {
			'succ':'<%=t1617;%>',
			'fail':'<%=t1618;%>',
			'btnedit':'<%=t1631;%>',
			'btnsure':'<%=t1628;%>'
		}
	</script>
  <script type="text/javascript" src="./../../js/manage/game_manage/cash_condition.js"></script>
</body>
</html>