<!DOCTYPE html>
<html lang="en">
<head>
	    <meta charset="UTF-8">
	    <meta http-equiv="X-UA-Compatible" content="IE=edge">
	    <meta name="viewport" content="width=device-width, initial-scale=1">
	    <title><%=t1007;%></title>
	    <link rel="stylesheet" href="./../../css/bootstrap.css">
	    <link rel="stylesheet" href="./../../css/commonPC.css">
	    <link rel="stylesheet" href="./../../css/definePC.css">
		 <!--[if lt IE 9]>
		   <script src="./../../js/html5shiv.min.js"></script>
		   <script src="./../../js/respond.min.js"></script>
		 <![endif]-->
		 <script src="./../../js/jquery-1.11.3.js"></script>
		 <script src="./../../js/bootstrap.js"></script>
		 <script src="./../../js/arttemplate.js"></script>
		 <script src="./../../js/child_define.js"></script>
</head>
<body>
	<div class="content marginT30">
		<div class="mainbox homebox">
			<div class="panel panel-default">
			  <div class="panel-heading clearfloat">
			  	<%=t1080;%>
			  	<p class="fr"><%=t1688;%>：<span id="pages"></span>，<%=t1689;%>：<span id="rows"></span></p>
			  	</div>
			  <div class="panel-body tab-content">
			  	    <div role="tabpanel" class="tab-pane tablebox active" id="tablebox">
			  	    	<div class="tablebox">
			  				<table class="table table-bordered table-striped">
			  				  <thead class="bgf5">
			  				  	<tr>
									<th>UID</th>
			  				  		<th><%=t1088;%></th>
			  				  		<th><%=t1089;%></th>
			  				  		<th><%=t1090;%></th>
			  				  		<th>VIP</th>
			  				  		<th><%=t1091;%></th>
			  				  		<th><%=t1092;%></th>
			  				  		<th><%=t1093;%></th>
			  				  		<th><%=t1094;%></th>
			  				  		<th><%=t1095;%></th>
			  				  	</tr>
			  				  </thead>
			  				  <tbody id="ranking_body"></tbody>
			  				</table>
			  				<!-- 分页 -->
			  				<div class="clearfloat">
			  					<nav aria-label="Page navigation" class="fr textright define-pagination">
			  					    <input type="hidden" name="" id="pagecount" value=""><!--输出总页数-->
			  					    <input type="hidden" name="" id="nowpage" value=""><!--输出当前页数-->
			  					    <input type="hidden" name="" value="" id="pageurl"><!--跳转地址-->
			  					    <ul class="pagination"></ul>
			  					</nav>
			  				</div>
			  				<script type="text/html" id="ranking_data">
			  				    {{each chart as val}}
			  				    	<tr>
			  				    		<td>{{val.uid}}</td>
				  				  		<td>{{val.nickname}}</td>
				  				  		<td>{{val.accountType}}</td>
				  				  		<td>{{val.rank}}</td>
				  				  		<td>{{val.vipLevel}}</td>
				  				  		<td>{{val.rechargeAmount}}</td>
				  				  		<td>{{val.cashAmount}}</td>
				  				  		<td>{{val.gold}}</td>
				  				  		<td>{{val.signupTime}}</td>
				  				  		<td>{{val.recentLoginTime}}</td>
				  				  	</tr>
			  				    {{/each}}
			  				 </script>
			  			</div>
			  	    </div>

			  </div>
			</div>
		</div>
	</div>
<script type="text/javascript" src="./../../js/manage/count/recharge_ranking.js"></script>
</body>
</html>